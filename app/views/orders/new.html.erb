<%= form_for :order, url: orders_path do |f| %>
<p>
<%= f.label :wine1_Qty  %><br>
<%= f.text_field :qty1 %>
</p>
<p>
<%= f.label :wine2_Qty %><br>
<%= f.text_field :qty2 %>
</p>
<p>
<%= f.label :name %><br>
<%= f.text_field :name %>
</p>
<p>
<%= f.label :address %><br>
<%= f.text_field :address %>
</p>
<%= f.submit %>
<% end %>